<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.eclipse.dltk.tcl.core.indexer.DLTKIndexerApplication">
         </run>
      </application>
   </extension>
   <extension
         id="indexer_product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.dltk.tcl.core.indexer.application"
            name="DLTK TCL Indexer">
         <property
               name="appName"
               value="DLTK TCL Indexer">
         </property>
      </product>
   </extension>

</plugin>
