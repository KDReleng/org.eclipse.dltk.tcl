<?xml version="1.0" encoding="UTF-8"?>
<templates>

<template name="%tclfile.template.name" id="org.eclipse.dltk.tcl.text.templates.tcl" description="%tclfile.template.description" context="org.eclipse.dltk.tcl.text.template.type.tcl" enabled="true">#!/bin/sh
# ${file_name} \
exec tclsh "$$0" $${1+"$$@"}
</template>

