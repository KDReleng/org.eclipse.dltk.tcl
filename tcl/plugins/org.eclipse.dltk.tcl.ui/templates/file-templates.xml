# ${file_name} \
exec wish "$$0" $${1+"$$@"}
</template>

<template name="%tclfile.template.env.name" id="org.eclipse.dltk.tcl.text.templates.tcl.env" description="%tclfile.template.env.description" context="org.eclipse.dltk.tcl.text.template.type.tcl" enabled="true">#!/bin/env tclsh
</template>

</templates>
